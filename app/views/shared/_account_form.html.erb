  <%= auth_token %>

  <label for="user_email">Email</label>
  <input
    id="user_email"
    name="user[email]"
    type="text"
    value="<%= @user.email %>">
  <br>

  <label for="user_password">Password</label>
  <input
    id="user_password"
    name="user[password]"
    type="password">
  <br>